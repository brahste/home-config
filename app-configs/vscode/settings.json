{
  "terminal.integrated.fontFamily": "'Hack Nerd Font'",
  "workbench.colorTheme": "Terra - Summer [Night]",
  "editor.minimap.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "python.formatting.provider": "none",
  "[lua]": {
    "editor.defaultFormatter": "yinfei.luahelper"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {
      "editorInlayHint.parameterForeground": "#806969",
      "editorInlayHint.typeForeground": "#807e69",
      "editorInlayHint.foreground": "#A77777"
  },
  "rust-analyzer.cargo.buildScripts.overrideCommand": "clippy",
  "nix.formatterPath": "nixfmt",
  "nix.serverPath": "nil",
  "nix.enableLanguageServer": true,
  "window.menuBarVisibility": "toggle",
  "explorer.confirmDelete": false,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-b>": false,
    "<C-w>": false
  },
  "cmake.configureOnOpen": true
}
